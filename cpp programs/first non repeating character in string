#include <iostream>
#include <string>
using namespace std;
 
int main()
{
    string s;
    int n;
    getline(cin, s);
    int l=s.length();
    int s1[127]={0};
    for(int i=0;i<l;i++){
        s1[s[i]]++;
    }
    for(int i=0;i<l;i++){
        if(s1[s[i]]==1){
            cout<<s[i];
            break;
        }
    }
    
    
    
}
