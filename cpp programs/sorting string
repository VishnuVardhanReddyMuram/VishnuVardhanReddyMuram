#include<stdio.h>
#include<iostream>
#include<string.h>
using namespace std;
int main(){
    string s;
    cin>>s;
     for(int i=0;i<s.length();i++){
        char c;
        for(int j=1;j<s.length();j++){
            if(s[i]>s[j]){
                c=s[i];
                s[i]=s[j];
                s[j]=c;
            }
        }
     }
     cout<<s;
    
}
