#include <iostream>
#include <string>
using namespace std;
bool checkanagram(string a,string b){
    int count1[123]={0};
    int count2[123]={0};
    for(int i=0;a[i] && b[i];i++){
        count1[a[i]]++;
        count2[b[i]]++;
    }
    for(int i=0;i<123;i++){
        if(count1[i]!=count2[i]){
            return false;
        }
    }
     return true;
}
int main()
{
    string s1,s2;
    cin>>s1;
    cin>>s2;
    
    if(checkanagram(s1,s2)){
        cout<<"the two strings are anagrams of each other";
    }
    else{
        cout<<"not anagrams of each other";
    }
    
    
    
    
    
}
